#installing python on new mac
kelsiowest@Kelsis-MacBook-Pro ~ % /bin/bash
bash-3.2$  /bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)"
bash-3.2$ (echo; echo 'eval "$(/opt/homebrew/bin/brew shellenv)"') >> /Users/kelsiowest/.zprofile
bash-3.2$     eval "$(/opt/homebrew/bin/brew shellenv)"
#open new terminal
kelsiowest@Kelsis-MacBook-Pro ~ % (echo; echo 'eval "$(/opt/homebrew/bin/brew shellenv)"') >> /Users/kelsiowest/.zprofile
    eval "$(/opt/homebrew/bin/brew shellenv)"
kelsiowest@Kelsis-MacBook-Pro ~ % brew install python
kelsiowest@Kelsis-MacBook-Pro ~ % python3 --version  
Python 3.9.6
